{
  "name": "cv",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start:prod": "concurrently \"npm run start:ssr:prod\" \"npm run start:api:prod\"",
    "start": "concurrently \"npm run start:ssr\" \"npm run start:api\"",
    "start:ssr": "cross-env NODE_ENV=development nodemon --watch dist/ssr.js --watch public ./dist/ssr.js",
    "start:browser": "webpack serve --mode development --config browser/webpack.config.js",
    "start:api": "cross-env NODE_ENV=development nodemon --watch dist/api.js ./dist/api.js",
    "start:ssr:prod": "cross-env NODE_ENV=production node ./dist/ssr.js",
    "start:api:prod": "cross-env NODE_ENV=production node ./dist/api.js",
    "build": "webpack --mode development",
    "build:prod": "webpack --mode production",
    "build:api": "webpack --mode development --config api/webpack.config.js",
    "build:browser": "webpack --mode development --config browser/webpack.config.js",
    "build:ssr": "webpack --mode development --config ssr/webpack.config.js",
    "watch": "webpack --watch --mode development",
    "watch:api": "webpack --watch --mode development --config api/webpack.config.js",
    "watch:browser": "webpack --watch --mode development --config browser/webpack.config.js",
    "watch:ssr": "webpack --watch --mode development --config ssr/webpack.config.js",
    "lint:browser": "tslint --config browser/tslint.json \"browser/src/**/*.{ts,tsx}\"",
    "lint:api": "tslint --config api/tslint.json \"api/src/**/*.ts\"",
    "lint:ssr": "tslint --config ssr/tslint.json \"ssr/src/**/*.{ts,tsx}\"",
    "lint": "npm run lint:browser && npm run lint:api && npm run lint:ssr",
    "test": "jest"
  },
  "dependencies": {
    "@emotion/cache": "^11.7.1",
    "@emotion/react": "^11.7.1",
    "@emotion/server": "^11.4.0",
    "@emotion/styled": "^11.6.0",
    "@mui/icons-material": "^5.2.5",
    "@mui/lab": "^5.0.0-alpha.63",
    "@mui/material": "^5.2.7",
    "@reduxjs/toolkit": "^1.7.1",
    "body-parser": "^1.19.0",
    "connected-react-router": "^6.9.1",
    "cookie-parser": "^1.4.5",
    "express": "^4.17.1",
    "history": "^4.7.2",
    "isomorphic-fetch": "^3.0.0",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.22.5",
    "react-redux": "^7.2.4",
    "react-router": "^5.2.0",
    "react-router-config": "^5.1.1",
    "react-router-dom": "^5.2.0",
    "react-transition-group": "^4.4.2",
    "redux": "^4.1.0",
    "redux-devtools-extension": "^2.13.9",
    "redux-observable": "^2.0.0",
    "remote-redux-devtools": "^0.5.16",
    "rxjs": "^7.2.0",
    "serve-favicon": "^2.5.0",
    "source-map-support": "^0.5.19",
    "three": "^0.130.1",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.5.5"
  },
