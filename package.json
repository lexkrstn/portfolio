{
  "name": "cv",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start:prod": "NODE_ENV=production npm run start:ssr:prod & npm run start:api:prod",
    "start": "concurrently \"npm run start:ssr\" \"npm run start:api\"",
    "start:ssr": "cross-env NODE_ENV=development nodemon --watch dist/ssr.js --watch public ./dist/ssr.js",
    "start:browser": "webpack serve --mode development --config browser/webpack.config.js",
    "start:api": "cross-env NODE_ENV=development nodemon --watch dist/api.js ./dist/api.js",
    "start:ssr:prod": "NODE_ENV=production node ./dist/ssr.js",
    "start:api:prod": "NODE_ENV=production node ./dist/api.js",
    "build": "webpack --mode development",
    "build:prod": "NODE_ENV=production webpack --mode production",
    "build:api": "webpack --mode development --config api/webpack.config.js",
    "build:api:prod": "NODE_ENV=production webpack --mode production --config api/webpack.config.js",
    "build:browser": "webpack --mode development --config browser/webpack.config.js",
    "build:browser:prod": "NODE_ENV=production webpack --mode production --config browser/webpack.config.js",
    "build:ssr": "webpack --mode development --config ssr/webpack.config.js",
    "build:ssr:prod": "webpack --mode production --config ssr/webpack.config.js",
    "watch": "webpack --watch --mode development",
    "watch:api": "webpack --watch --mode development --config api/webpack.config.js",
    "watch:browser": "webpack --watch --mode development --config browser/webpack.config.js",
    "watch:ssr": "webpack --watch --mode development --config ssr/webpack.config.js",
    "lint:browser": "eslint --config browser/.eslintrc.js \"browser/src/**/*.{ts,tsx}\"",
    "lint:api": "eslint --config api/.eslintrc.js \"api/src/**/*.ts\"",
    "lint:ssr": "eslint --config ssr/.eslintrc.js \"ssr/src/**/*.{ts,tsx}\"",
    "lint": "npm run lint:browser && npm run lint:api",
    "test": "npm run test:api && npm run test:api:e2e",
    "test:api": "jest --config ./api/jest.config.js",
    "test:api:watch": "jest --watch --config ./api/jest.config.js",
    "test:api:coverage": "jest --coverage --config ./api/jest.config.js",
    "test:api:e2e": "jest --config ./api/test/jest.config.js",
    "test:api:e2e:watch": "jest --watch --config ./api/test/jest.config.js",
    "test:browser": "jest --config ./browser/jest.config.js",
    "test:browser:watch": "jest --watch --config ./browser/jest.config.js",
    "test:browser:coverage": "jest --coverage --config ./browser/jest.config.js"
  },
