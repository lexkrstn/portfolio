{
  "name": "cv",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start:prod": "concurrently \"npm run start:ssr:prod\" \"npm run start:api:prod\"",
    "start": "concurrently \"npm run start:ssr\" \"npm run start:api\"",
    "start:ssr": "cross-env NODE_ENV=development nodemon --watch dist/ssr.js --watch public ./dist/ssr.js",
    "start:browser": "webpack serve --mode development --config webpack.config.browser.js",
    "start:api": "cross-env NODE_ENV=development nodemon --watch dist/api.js ./dist/api.js",
    "start:ssr:prod": "cross-env NODE_ENV=production node ./dist/ssr.js",
    "start:api:prod": "cross-env NODE_ENV=production node ./dist/api.js",
    "build": "webpack --mode development",
    "build:prod": "webpack --mode production",
    "build:api": "webpack --mode development --config webpack.config.api.js",
    "build:browser": "webpack --mode development --config webpack.config.browser.js",
    "build:ssr": "webpack --mode development --config webpack.config.ssr.js",
    "watch": "webpack --watch --mode development",
    "watch:api": "webpack --watch --mode development --config webpack.config.api.js",
    "watch:browser": "webpack --watch --mode development --config webpack.config.browser.js",
    "watch:ssr": "webpack --watch --mode development --config webpack.config.ssr.js",
    "lint:browser": "tslint --config src/browser/tslint.json \"src/browser/**/*.{ts,tsx}\"",
    "lint:api": "tslint --config src/api/tslint.json \"src/api/**/*.ts\"",
    "lint:ssr": "tslint --config src/ssr/tslint.json \"src/ssr/**/*.{ts,tsx}\"",
    "lint": "concurrently \"npm run lint:browser\"  \"npm run lint:api\" \"npm run lint:ssr\"",
    "test": "jest"
  },
  "dependencies": {
    "@styled-icons/fa-brands": "^10.34.0",
    "body-parser": "^1.19.0",
    "connected-react-router": "^6.9.1",
    "cookie-parser": "^1.4.5",
    "express": "^4.17.1",
    "history": "^4.10.1",
    "isomorphic-fetch": "^3.0.0",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "polished": "^4.1.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.4",
    "react-router": "^5.2.0",
    "react-router-config": "^5.1.1",
    "react-router-dom": "^5.2.0",
    "react-transition-group": "^4.4.2",
    "redux": "^4.1.0",
    "redux-devtools-extension": "^2.13.9",
    "redux-observable": "^2.0.0",
    "remote-redux-devtools": "^0.5.16",
    "serve-favicon": "^2.5.0",
    "source-map-support": "^0.5.19",
    "styled-components": "^5.3.0",
    "three": "^0.130.1",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.5.5"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.12",
    "@types/jest": "^26.0.23",
    "@types/lodash": "^4.14.170",
    "@types/morgan": "^1.9.2",
    "@types/react": "^17.0.11",
    "@types/react-dom": "^17.0.8",
    "@types/react-redux": "^7.1.16",
    "@types/react-router": "^5.1.15",
    "@types/react-router-config": "^5.0.2",
    "@types/react-router-dom": "^5.1.7",
    "@types/react-transition-group": "^4.4.1",
    "@types/remote-redux-devtools": "^0.5.4",
    "@types/serve-favicon": "^2.5.2",
    "@types/styled-components": "^5.1.10",
    "@types/three": "^0.130.0",
    "@types/winston": "^2.4.4",
    "concurrently": "^6.2.0",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.3.2",
    "jest": "^27.0.5",
    "node-flag": "~1.5.1",
    "nodemon": "^2.0.7",
    "pug": "^2.0.4",
    "pug-loader": "^2.4.0",
    "style-loader": "^3.0.0",
    "terser-webpack-plugin": "^5.1.4",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "tslint": "^5.20.1",
    "tslint-react": "^4.1.0",
    "typescript": "^4.3.4",
    "typescript-plugin-styled-components": "^1.6.0",
    "typescript-tslint-plugin": "^1.0.1",
    "url-loader": "^4.1.1",
    "webpack": "^5.40.0",
    "webpack-bundle-analyzer": "^4.4.2",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2",
    "webpack-node-externals": "^3.0.0"
  }
}
